if ((pow(contours[i][j].x - 30, 2) + pow(contours[i][j].y + 30, 2) < 36)) break;																							//Wewnąrz pierwszego okręgu
							else if ((pow(contours[i][j].x - 30, 2) + pow(contours[i][j].y + 30, 2)) > 36 && ((pow(contours[i][j].x - 30, 2) + pow(contours[i][j].y + 30, 2)) < 81)) fill[0]++;			//Między pierwszym a drugim okręgiem
							else if ((pow(contours[i][j].x - 30, 2) + pow(contours[i][j].y + 30, 2)) > 36 && ((pow(contours[i][j].x - 30, 2) + pow(contours[i][j].y + 30, 2)) < 81)) fill[1]++;			//Między drugim a trzecim okręgiem
							else if ((pow(contours[i][j].x - 30, 2) + pow(contours[i][j].y + 30, 2)) > 81 && ((pow(contours[i][j].x - 30, 2) + pow(contours[i][j].y + 30, 2)) < 144)) fill[2]++;		//Między trzecim a czwartym okręgiem
							else if ((pow(contours[i][j].x - 30, 2) + pow(contours[i][j].y + 30, 2)) > 144 && ((pow(contours[i][j].x - 30, 2) + pow(contours[i][j].y + 30, 2)) < 225)) fill[3]++;		//Między czwartym a piątym okręgiem
